<%- include('header') %>

<div class="body-content">
    <div class="card-header">
        <h6 class="fs-17 fw-semi-bold mb-0">Edit WhatsApp Settings</h6>
    </div>

    <div class="card mb-4">
        <div class="card-body">

          
            <form action="/whats/<%=whats.id%>" method="POST">
               
             
                <div class="bg-light p-3 my-2">
                   

                    <div class="mb-3">
                        <label for="token" class="form-label">token</label>
                        <input type="text" class="form-control" id="token" name="token" placeholder="Enter token"  value="<%=whats.token%>" required>
                    </div>

                    <div class="mb-3">
                        <label for="client" class="form-label">client</label>
                        <input type="text" class="form-control" id="client" name="client"  placeholder="Enter Client" value="<%=whats.client%>" >
                    </div>

                            </div>
                   
                    </div>
                </div>

             
                <div class="mt-3">
                    <button type="submit" class="btn btn-success">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>

</body>
<script src="js/metisMenu/metisMenu.min.js"></script>
<script src="js/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
<!-- message hide -->
<script src="js/sidebar.js"></script>
<script src="js/common.js"></script>

    

</html>