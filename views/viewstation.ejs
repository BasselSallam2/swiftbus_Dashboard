                                    <a><%=station[i].name%></a>
                                </td>
                                <td>
                                    <a><%=station[i].Arabicname%></a>
                                </td>
                                <td><%=station[i].city.name%></td>
                                <td><%=station[i].address%></td>
                                <td><%=station[i].location%></td>
                               

                                <td>



                                    <a href="/station/<%=station[i].id%>" class="btn btn-sm btn-info text-white" title="Edit">
                                        <i class="fas fa-edit"></i>
                                    </a>
                                    <a href="/station/delete/<%=station[i].id%>" class="btn btn-sm btn-danger text-white" title="delete">
                                        <i class="fas fa-trash"></i>
                                    </a>
                                </td>
