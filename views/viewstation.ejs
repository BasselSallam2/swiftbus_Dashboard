                                <th scope="col">Arabic Name</th>
                                <th scope="col">City</th>
                                <th scope="col">Address</th>
                                <th scope="col">Location</th>
                                <th scope="col">Action</th>

                            
                                <% for(let i = 0 ; i < station.length ; i++) { %>
                                       

                            </tr>
                        </thead>
